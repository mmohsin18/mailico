{
  "id": "/",
  "name": "Mailico",
  "short_name": "Mailico",
  "description": "Your Emailing Buddy",
  "lang": "en-US",
  "dir": "ltr",
  "scope": "/inbox",
  "start_url": "/inbox",
  "display": "standalone",
  "share_target": {
    "action": "/add",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "display_override": ["standalone", "minimal-ui"],
  "theme_color": "#FF4D2E",
  "offline_enabled": true,
  "offline_page": "/offline",
  "splash_pages": "/splash",
  "background_color": "#0F0F10",
  "orientation": "portrait",
  "icons": [
    {
      "src": "/favicon/icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/favicon/icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "prefer_related_applications": false
}
